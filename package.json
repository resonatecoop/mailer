{
  "name": "resonate-contact",
  "version": "1.0.0",
  "description": "Contact Form Backend Microservice",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "node server.js",
    "dev": "babel-node -r ./node_modules/dotenv-safe/config ./server.dev.js",
